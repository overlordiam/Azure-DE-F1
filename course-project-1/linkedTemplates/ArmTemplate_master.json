{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "course-project-1"
		},
		"Azure_f1_dl_pl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Azure_f1_dl_pl'"
		},
		"Azure_f1_dl_pl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeforprojformula1.dfs.core.windows.net/"
		},
		"databricks_ls_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0723-192459-6ez2brzf"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "course-project-1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Azure_f1_dl_pl_accountKey": {
						"value": "[parameters('Azure_f1_dl_pl_accountKey')]"
					},
					"Azure_f1_dl_pl_properties_typeProperties_url": {
						"value": "[parameters('Azure_f1_dl_pl_properties_typeProperties_url')]"
					},
					"databricks_ls_properties_typeProperties_existingClusterId": {
						"value": "[parameters('databricks_ls_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}